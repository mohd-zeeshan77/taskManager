{
  "version": 2,
  "builds": [
    {
      "src": "target/taskmanager.jar",
      "use": "@vercel/java",
      "config": {
        "main": "com.example.TaskManagerApplication"
      }
    }
  ],
  "routes": [
    {
      "src": "/taskmanager/(.*)",
      "dest": "target/taskmanager.jar"
    },
    {
      "src": "/(.*)",
      "dest": "target/taskmanager.jar"
    }
  ]
}
